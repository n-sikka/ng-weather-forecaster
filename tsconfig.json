{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@app/shared/": ["./src/app/modules/shared/"],
      "@app/feature/": ["./src/app/modules/feature/"],
      "@app/shared/ui/": ["./src/app/modules/shared/ui/ui.module"],
      "@app/shared/core/": ["./src/app/modules/shared/core/core.module"],
      "@app/shared/service/": ["./src/app/modules/shared/service/service.module"],
      "@app/feature/weather/": ["./src/app/modules/feature/weather/weather.module"],
      "@app/feature/home/": ["./src/app/modules/feature/home/home.module"]
    }
  }
}
